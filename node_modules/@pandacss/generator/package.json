{
  "name": "@pandacss/generator",
  "version": "0.10.0",
  "description": "The css generator for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "javascript-stringify": "2.1.0",
    "lil-fp": "1.4.5",
    "outdent": " ^0.8.0",
    "pluralize": "8.0.0",
    "postcss": "8.4.27",
    "ts-pattern": "5.0.4",
    "@pandacss/core": "0.10.0",
    "@pandacss/is-valid-prop": "0.10.0",
    "@pandacss/logger": "0.10.0",
    "@pandacss/shared": "0.10.0",
    "@pandacss/token-dictionary": "0.10.0",
    "@pandacss/types": "0.10.0"
  },
  "devDependencies": {
    "@types/pluralize": "0.0.30",
    "hookable": "5.5.3",
    "@pandacss/fixture": "0.10.0"
  },
  "scripts": {
    "prebuild": "tsx scripts/prebuild.ts",
    "build": "pnpm prebuild && tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "tsx scripts/prebuild.ts && pnpm build-fast --watch"
  }
}